#COMANDO DE DESVIO INCODICIONAL

PROGRAM DIVISION.
MAIN.
PIC9 X VALUE 0.
PIC9 Y VALUE 0.
PICBOOL verdade VALUE TRUE.

PERFORM UNTIL (X < 100).
	ACCEPT X FROM COMPUTE(X+1).
	IF (X=42) THEN
		CONTINUE.
	ENDIF
	ACCEPT Y FROM COMPUTE(Y-1).
ENDPERFORM

PERFORM UNTIL TRUE.
	DISPLAY X.
	ACCEPT X FROM COMPUTE(X+1).
	IF (X>42) THEN
		BREAK.
	ENDIF 
ENDPERFORM

ENDMAIN